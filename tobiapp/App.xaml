<Application x:Class="Tobi.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:Properties="clr-namespace:Tobi.Properties" xmlns:XAML="clr-namespace:Tobi.Common.UI.XAML;assembly=Tobi.Common" xmlns:Tobi="clr-namespace:Tobi" Startup="OnApplicationStartup"
             ShutdownMode="OnExplicitShutdown">

  <Application.Resources>
    <ResourceDictionary>
      <Tobi:Settings x:Key="Settings"/>
      <!-- XAML:WindowStateFullScreenConverter x:Key="WindowStateFullScreenConverter" / -->
      
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/Tobi.Infrastructure;component/IconResources.xaml"/>
        <ResourceDictionary Source="pack://application:,,,/Tobi.Common;component/Resources.xaml"/>
       </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>
</Application>
