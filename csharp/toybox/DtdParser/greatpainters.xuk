																				<XmlAttributes>
																					<XmlAttribute
																						LocalName="id"
																						Value="dtb60" />
																				</XmlAttributes>
																			</XmlProperty>
																		</Properties>
																		<Children>
																			<TreeNode>
																				<Properties>
																					<ChannelsProperty>
																						<ChannelMappings>
																							<ChannelMapping
																								Channel="CH00000">
																								<TextMedia>
																									<Text> 1 Dante, </Text>
																								</TextMedia>
																							</ChannelMapping>
																						</ChannelMappings>
																					</ChannelsProperty>
																				</Properties>
																				<Children />
