														<Properties>
															<XmlProperty
																LocalName="p" />
														</Properties>
														<Children>
															<TreeNode>
																<Properties>
																	<XmlProperty
																		LocalName="sent">
																		<XmlAttributes>
																			<XmlAttribute
																				LocalName="id"
																				Value="dtb77" />
																		</XmlAttributes>
																	</XmlProperty>
																	<ChannelsProperty>
																		<ChannelMappings>
																			<ChannelMapping
																				Channel="CH00000">
																				<TextMedia>
																					<Text>It is related that the Pope, scandalized at the nudity of certain figures, a nudity which Daniele da Volterra was afterwards charged to clothe, sent word to Michael Angelo that he must cover them.</Text>
																				</TextMedia>
																			</ChannelMapping>
																		</ChannelMappings>
																	</ChannelsProperty>
																</Properties>
																<Children />
