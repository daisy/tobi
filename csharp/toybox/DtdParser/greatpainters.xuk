																			<ChannelsProperty>
																				<ChannelMappings>
																					<ChannelMapping
																						Channel="CH00000">
																						<TextMedia>
																							<Text>colouring</Text>
																						</TextMedia>
																					</ChannelMapping>
																				</ChannelMappings>
																			</ChannelsProperty>
																		</Properties>
