																		<Properties>
																			<ChannelsProperty>
																				<ChannelMappings>
																					<ChannelMapping
																						Channel="CH00000">
																						<TextMedia>
																							<Text> Among these representatives of high finance a painter might find a choice </Text>
																						</TextMedia>
																					</ChannelMapping>
																				</ChannelMappings>
																			</ChannelsProperty>
																		</Properties>
