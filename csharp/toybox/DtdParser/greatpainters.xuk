																<Properties>
																	<XmlProperty
																		LocalName="sent">
																		<XmlAttributes>
																			<XmlAttribute
																				LocalName="id"
																				Value="dtb198" />
																		</XmlAttributes>
																	</XmlProperty>
																	<ChannelsProperty>
																		<ChannelMappings>
																			<ChannelMapping
																				Channel="CH00000">
																				<TextMedia>
																					<Text> In painting a picture they proceeded on the following system. </Text>
																				</TextMedia>
																			</ChannelMapping>
																		</ChannelMappings>
																	</ChannelsProperty>
																</Properties>
																<Children />
