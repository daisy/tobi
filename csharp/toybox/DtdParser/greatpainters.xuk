														</Children>
													</TreeNode>
													<TreeNode>
														<Properties>
															<XmlProperty
																LocalName="pagenum">
																<XmlAttributes>
																	<XmlAttribute
																		LocalName="id"
																		Value="page10" />
																	<XmlAttribute
																		LocalName="page"
																		Value="normal" />
																</XmlAttributes>
															</XmlProperty>
															<ChannelsProperty>
																<ChannelMappings>
																	<ChannelMapping
																		Channel="CH00000">
																		<TextMedia>
																			<Text>10</Text>
																		</TextMedia>
																	</ChannelMapping>
																</ChannelMappings>
															</ChannelsProperty>
														</Properties>
														<Children />
