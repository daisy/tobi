																<Properties>
																	<XmlProperty
																		LocalName="sent">
																		<XmlAttributes>
																			<XmlAttribute
																				LocalName="id"
																				Value="dtb192" />
																		</XmlAttributes>
																	</XmlProperty>
																	<ChannelsProperty>
																		<ChannelMappings>
																			<ChannelMapping
																				Channel="CH00000">
																				<TextMedia>
																					<Text> Oil painting, it is true, had long been in use, but only in a very undeveloped form, and for inferior purposes. </Text>
																				</TextMedia>
																			</ChannelMapping>
																		</ChannelMappings>
																	</ChannelsProperty>
																</Properties>
