															<TreeNode>
																<Properties>
																	<XmlProperty
																		LocalName="sent">
																		<XmlAttributes>
																			<XmlAttribute
																				LocalName="id"
																				Value="dtb138" />
																		</XmlAttributes>
																	</XmlProperty>
																	<ChannelsProperty>
																		<ChannelMappings>
																			<ChannelMapping
																				Channel="CH00000">
																				<TextMedia>
																					<Text> Why this thought? </Text>
																				</TextMedia>
																			</ChannelMapping>
																		</ChannelMappings>
																	</ChannelsProperty>
																</Properties>
																<Children />
															</TreeNode>
															<TreeNode>
																<Properties>
																	<XmlProperty
																		LocalName="sent">
																		<XmlAttributes>
																			<XmlAttribute
																				LocalName="id"
																				Value="dtb139" />
																		</XmlAttributes>
																	</XmlProperty>
																	<ChannelsProperty>
																		<ChannelMappings>
																			<ChannelMapping
																				Channel="CH00000">
																				<TextMedia>
																					<Text> Is it purely emblematic, or does it contain an allusion to some private matter? </Text>
																				</TextMedia>
																			</ChannelMapping>
																		</ChannelMappings>
																	</ChannelsProperty>
																</Properties>
																<Children />
															</TreeNode>
															<TreeNode>
																<Properties>
																	<XmlProperty
																		LocalName="sent">
																		<XmlAttributes>
																			<XmlAttribute
																				LocalName="id"
																				Value="dtb140" />
																		</XmlAttributes>
																	</XmlProperty>
																	<ChannelsProperty>
																		<ChannelMappings>
																			<ChannelMapping
																				Channel="CH00000">
																				<TextMedia>
																					<Text> We are led to believe that it is intended as a complementary explanation, that it was placed upon the picture because it was in sympathy with a train of ideas special to the model. </Text>
																				</TextMedia>
																			</ChannelMapping>
																		</ChannelMappings>
																	</ChannelsProperty>
																</Properties>
																<Children />
