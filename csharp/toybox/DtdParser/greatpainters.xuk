														<Properties>
															<XmlProperty
																LocalName="note">
																<XmlAttributes>
																	<XmlAttribute
																		LocalName="id"
																		Value="footnote-1" />
																	<XmlAttribute
																		LocalName="class"
																		Value="Footnote" />
																</XmlAttributes>
															</XmlProperty>
														</Properties>
														<Children>
