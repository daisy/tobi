																	<ChannelsProperty>
																		<ChannelMappings>
																			<ChannelMapping
																				Channel="CH00000">
																				<TextMedia>
																					<Text> This noble work is certified by an inscription. </Text>
																				</TextMedia>
																			</ChannelMapping>
																		</ChannelMappings>
																	</ChannelsProperty>
																</Properties>
																<Children />
															</TreeNode>
															<TreeNode>
																<Properties>
																	<XmlProperty
																		LocalName="sent">
																		<XmlAttributes>
																			<XmlAttribute
																				LocalName="id"
																				Value="dtb206" />
																		</XmlAttributes>
																	</XmlProperty>
																	<ChannelsProperty>
																		<ChannelMappings>
																			<ChannelMapping
																				Channel="CH00000">
																				<TextMedia>
																					<Text> It is a large altar picture, consisting of two rows of separate panels, once in the Cathedral of St. </Text>
																				</TextMedia>
																			</ChannelMapping>
																		</ChannelMappings>
																	</ChannelsProperty>
																</Properties>
																<Children />
