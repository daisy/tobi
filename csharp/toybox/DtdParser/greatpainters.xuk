																		<XmlAttributes>
																			<XmlAttribute
																				LocalName="id"
																				Value="dtb199" />
																		</XmlAttributes>
																	</XmlProperty>
																</Properties>
																<Children>
																	<TreeNode>
																		<Properties>
																			<ChannelsProperty>
																				<ChannelMappings>
																					<ChannelMapping
																						Channel="CH00000">
																						<TextMedia>
																							<Text> The outline was drawn on a </Text>
																						</TextMedia>
																					</ChannelMapping>
																				</ChannelMappings>
																			</ChannelsProperty>
																		</Properties>
																		<Children />
