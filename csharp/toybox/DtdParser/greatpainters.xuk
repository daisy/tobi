																		<XmlAttributes>
																			<XmlAttribute
																				LocalName="id"
																				Value="dtb123" />
																		</XmlAttributes>
																	</XmlProperty>
																	<ChannelsProperty>
																		<ChannelMappings>
																			<ChannelMapping
																				Channel="CH00000">
																				<TextMedia>
																					<Text> When we saw this masterly work in the Museum of Berlin, to which it belongs, it left an indelible impression upon us which we still feel at this distance. </Text>
																				</TextMedia>
																			</ChannelMapping>
																		</ChannelMappings>
																	</ChannelsProperty>
																</Properties>
																<Children />
