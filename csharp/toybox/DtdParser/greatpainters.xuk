																	<XmlAttribute
																		LocalName="id"
																		Value="page16" />
																	<XmlAttribute
																		LocalName="page"
																		Value="normal" />
																</XmlAttributes>
															</XmlProperty>
															<ChannelsProperty>
																<ChannelMappings>
																	<ChannelMapping
																		Channel="CH00000">
																		<TextMedia>
																			<Text>16</Text>
																		</TextMedia>
																	</ChannelMapping>
																</ChannelMappings>
															</ChannelsProperty>
														</Properties>
														<Children />
