																<Properties>
																	<XmlProperty
																		LocalName="sent">
																		<XmlAttributes>
																			<XmlAttribute
																				LocalName="id"
																				Value="dtb303" />
																		</XmlAttributes>
																	</XmlProperty>
																	<ChannelsProperty>
																		<ChannelMappings>
																			<ChannelMapping
																				Channel="CH00000">
																				<TextMedia>
																					<Text> The aged Jodocus Vydts, to whose liberality posterity is indebted for this great work of art, is dressed in a simple red garment trimmed with fur; he kneels with his hands folded, and his eyes directed upwards. </Text>
																				</TextMedia>
																			</ChannelMapping>
																		</ChannelMappings>
																	</ChannelsProperty>
																</Properties>
																<Children />
