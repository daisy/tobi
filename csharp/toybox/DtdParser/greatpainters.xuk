																			<XmlProperty
																				LocalName="em" />
																			<ChannelsProperty>
																				<ChannelMappings>
																					<ChannelMapping
																						Channel="CH00000">
																						<TextMedia>
																							<Text>Paradiso</Text>
																						</TextMedia>
																					</ChannelMapping>
																				</ChannelMappings>
																			</ChannelsProperty>
																		</Properties>
																		<Children />
