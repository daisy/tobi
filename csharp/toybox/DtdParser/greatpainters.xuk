																	<ChannelsProperty>
																		<ChannelMappings>
																			<ChannelMapping
																				Channel="CH00000">
																				<TextMedia>
																					<Text> This immense work was exhibited to the public on Christmas Day, 1541. </Text>
																				</TextMedia>
																			</ChannelMapping>
																		</ChannelMappings>
																	</ChannelsProperty>
																</Properties>
