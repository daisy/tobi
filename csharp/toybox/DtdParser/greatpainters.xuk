																					<ChannelsProperty>
																						<ChannelMappings>
																							<ChannelMapping
																								Channel="CH00000">
																								<TextMedia>
																									<Text>Inferno</Text>
																								</TextMedia>
																							</ChannelMapping>
																						</ChannelMappings>
																					</ChannelsProperty>
																				</Properties>
																				<Children />
																			</TreeNode>
																			<TreeNode>
																				<Properties>
																					<ChannelsProperty>
																						<ChannelMappings>
																							<ChannelMapping
																								Channel="CH00000">
																								<TextMedia>
																									<Text> III. </Text>
																								</TextMedia>
																							</ChannelMapping>
																						</ChannelMappings>
																					</ChannelsProperty>
																				</Properties>
																				<Children />
